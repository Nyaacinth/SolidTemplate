{
    // General
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": { "source.organizeImports": "explicit" },
    // ESLint
    "eslint.options": { "ignorePath": ".gitignore" },
    // Prettier
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "prettier.ignorePath": ".gitignore"
}
